generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Website {
  id        Int      @id @default(autoincrement())
  url       String
  createdAt DateTime @default(now())
}

model TrackingEvent {
  id            Int      @id @default(autoincrement())
  type          String
  url           String
  element       String?
  elementId     String?
  elementClasses String?
  timestamp     DateTime
  createdAt     DateTime @default(now())
}